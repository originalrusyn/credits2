<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>${app.id}</application>
    <version>${app.version}</version>
    <threadsafe>true</threadsafe>

    <system-properties>
        <property name="log4j2.disable.jmx" value="true" />
        <property name="java.util.logging.manager" value="org.apache.logging.log4j.jul.LogManager" />
        <property name="appengine.user.timezone.impl" value="UTC"/>
        <property name="appengine.user.timezone" value="UTC"/>
    </system-properties>
</appengine-web-app>
